{
    "name": "phptopup/symfony-form",
    "description": "Symfony Form reference generator",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "^2.6",
        "symfony/console": "4.1.*",
        "symfony/doctrine-bridge": "4.1.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.1.*",
        "symfony/framework-bundle": "4.1.*",
        "symfony/http-foundation": "4.1.*",
        "symfony/http-kernel": "4.1.*",
        "symfony/intl": "4.1.*",
        "symfony/options-resolver": "4.1.*",
        "symfony/process": "4.1.*",
        "symfony/property-access": "4.1.*",
        "symfony/security-core": "4.1.*",
        "symfony/security-csrf": "4.1.*",
        "symfony/translation": "4.1.*",
        "symfony/validator": "4.1.*",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "4.1.*"
    },
    "require-dev": {
        "symfony/dotenv": "4.1.*",
        "symfony/var-dumper": "4.1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
